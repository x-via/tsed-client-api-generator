{
  "name": "tsed-client-api-generator",
  "version": "0.0.1",
  "description": "Automatically generates Typescript APIs from Ts.ED controllers",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.json && npx cpx \"templates/*.ejs\" ./lib/templates",
    "test": "jest --coverage",
    "release": "npm run build && npx semantic-release"
  },
  "dependencies": {},
  "private": false,
  "devDependencies": {
    "@tsed/common": "6.17.4",
    "@tsed/schema": "6.17.4",
    "@types/ejs": "^3.0.5",
    "@types/jest": "^26.0.19",
    "coveralls": "^3.1.0",
    "ejs": "^3.1.5",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,ts}": "prettier --write"
  },
  "files": [
    "/lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/x-via/tsed-client-api-generator.git"
  }
}
